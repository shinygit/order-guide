{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "test-server": "TEST=TEST nodemon --ignore 'Frontend/' --exec babel-node Backend/index.js",
    "test": "mocha --require @babel/register './**/*.spec.js'",
    "heroku-postbuild": "npm run build && cd Frontend && npm install --only=dev && npm install && npm run build",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel Backend --out-dir build",
    "build": "npm run build-babel",
    "test:cypress": "start-server-and-test test:start http://localhost:3000 cypress",
    "start-react-webserver": "cd Frontend && BROWSER=none npm start",
    "test:start": "concurrently \"npm run test-server\" \"npm run start-react-webserver\"",
    "cypress": "cypress run",
    "cypress:open": "cypress open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.16.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^52.1.0",
    "graphql": "^15.3.0",
    "graphql-resolvers": "^0.4.2",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.0",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^5.22.3",
    "uuid": "^8.3.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "cypress": "^4.12.1",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1",
    "start-server-and-test": "^1.11.3"
  },
  "engines": {
    "node": "14.2.0"
  }
